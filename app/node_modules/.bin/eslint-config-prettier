#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules/eslint-config-prettier/build/bin/node_modules:/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules/eslint-config-prettier/build/node_modules:/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules/eslint-config-prettier/node_modules:/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules:/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/node_modules"
else
  export NODE_PATH="/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules/eslint-config-prettier/build/bin/node_modules:/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules/eslint-config-prettier/build/node_modules:/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules/eslint-config-prettier/node_modules:/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules:/Users/liluanhui/Desktop/code/github/loki/node_modules/.pnpm/node_modules:$NODE_PATH"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules/eslint-config-prettier/build/bin/cli.js" "$@"
else
  exec node  "$basedir/../../../node_modules/.pnpm/eslint-config-prettier@10.0.2_eslint@9.21.0_jiti@2.4.2_/node_modules/eslint-config-prettier/build/bin/cli.js" "$@"
fi
